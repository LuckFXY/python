        <script type="text/javascript">
            function topOrder(container) {
                var parent = document.getElementById(container);
                var childs = parent.children;
                var childs_num = childs.length ;
                for(var i= 0 ; i< childs_num ; i++) {
                    var spanobj = childs[i].getElementsByTagName('span');
                    spanobj[0].innerHTML = i+1 ;
                }
            }
            topOrder('cttul1');
            topOrder('cttul2');
            topOrder('cttul3');
        </script>
        <script language="javascript" type="text/javascript">
            function descorder(k) {
                var p_obj = document.getElementById(k);
                var childs = p_obj.getElementsByTagName('li');
                var total = childs.length;
                for(var i = 0 ;total >0 ; i++ , total--) {
                    var temp_obj= childs[i].getElementsByTagName('table');
                    var tds = temp_obj[0].getElementsByTagName('td');
                    tds[2].innerHTML = total ;
                }
            }
            descorder("plbody");
        </script>
